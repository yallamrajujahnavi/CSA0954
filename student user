import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    int to,sta,stu,x,y;
		System.out.print("Total Users: ");
		to=sc.nextInt();
		System.out.print("Staff Users: ");
		sta=sc.nextInt();
		if(to<=0||sta<0||to<sta||to==sta)
		{
		    System.out.println("Invalid user entry");
		    return;
		}
		x=sta/3;
		y=sta+x;
		stu=to-y;
		System.out.print("Student Users: "+stu);
	}
}
